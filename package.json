{
  "name": "jiffy",
  "version": "0.0.1",
  "description": "Jiffy Web",
  "author": "Shubham Singh <imshubhamsingh@gmail.com>",
  "license": "GNU",
  "scripts": {
    "build": "yarn format && yarn typecheck && yarn lint && webpack --config config/webpack/webpack.prod.js",
    "watch": "webpack-dev-server --mode development --config config/webpack/webpack.dev.js --port 3000 --open",
    "prod": "ts-node server.ts",
    "typecheck": "tsc --noEmit --pretty",
    "format": "prettier --write \"{src,__tests__}/**/*.{ts,tsx}\"",
    "lint": "tslint --force --format verbose \"src/**/*.{ts,tsx}\""
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@emotion/core": "^10.0.5",
    "@emotion/styled": "^10.0.5",
    "emotion": "^10.0.5",
    "emotion-theming": "^10.0.5",
    "express": "^4.16.4",
    "express-history-api-fallback": "^2.2.1",
    "react": "^16.7.0-alpha.2",
    "react-dom": "^16.7.0-alpha.2",
    "react-router-dom": "^4.4.0-beta.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-react-inline-elements": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
